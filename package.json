{
  "name": "ccxws",
  "version": "0.25.0",
  "description": "Websocket client for 20+ cryptocurrency exchanges",
  "keywords": [
    "bitmex",
    "binance",
    "coinbase",
    "cryptocurrency",
    "exchange",
    "websocket",
    "realtime"
  ],
  "author": "Brian Mancini <bmancini@gmail.com>",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "test:mocha": "mocha --recursive \"__tests__/**/*.spec.js\"",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:mocha",
    "test": "jest --testEnvironment=node --testRunner=jest-circus/runner --coverage --testPathPattern=src/.+\\.spec\\.js --maxWorkers=32",
    "test:ci": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": "altangent/ccxws",
  "dependencies": {
    "cloudscraper": "^1.5.1",
    "decimal.js": "^10.1.1",
    "jsonic": "^0.3.1",
    "moment": "^2.24.0",
    "pusher-js": "^4.4.0",
    "semaphore": "^1.1.0",
    "signalr-client": "0.0.17",
    "winston": "^2.4.4",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "jest": "^24.5.0",
    "jest-circus": "^24.5.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.16.4",
    "sinon": "^7.3.2"
  }
}
